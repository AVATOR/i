<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="myProcess" name="Кузнецьовський МВК - Звернення про надання дозволу на проведення публічних богослужінь" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="orgName" name="Назва релігійної громади/організації" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="eventName" name="Назва заходу" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="eventDate" name="Дата проведення заходу" type="date" required="true"></activiti:formProperty>
        <activiti:formProperty id="eventPlace" name="Місце проведення заходу" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="eventTime" name="Час проведення заходу" type="string"></activiti:formProperty>
        <activiti:formProperty id="eventEntertainment" name="Заход включатиме: " type="string"></activiti:formProperty>
        <activiti:formProperty id="eventSize" name="Очікувана кількість присутніх" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище організатора-громадянина" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я організатора-громадянина" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькове" type="string"></activiti:formProperty>
        <activiti:formProperty id="dateCreation" name="Дата заповнення заяви" type="date" required="true"></activiti:formProperty>
        <activiti:formProperty id="orgEmail" name="Емейл адреса організатора " type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="contactPhone" name="Контактний номер телефона" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="contactAddress" name="Контактна адреса організатора" type="string"></activiti:formProperty>
        <activiti:formProperty id="needPhysicalCopy?" name="Чи потрібна Вам фізична копія документа?" type="enum" required="true">
          <activiti:value id="digital" name="Тільки Електронна версія"></activiti:value>
          <activiti:value id="physical" name="Фізична+Електронна копія  "></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="mailtask1" name="Емейл підтвердження " activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${orgEmail}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Звернення щодо: ${eventName}</activiti:expression>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string>UTF-8</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>Шановний громадянин, ${bankIdfirstName} ${bankIdmiddleName}

Ваша заява була успішно прийнята в роботу. Відповідь повинна бути протягом 10 діб.</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="usertask1" name="Заява поступає в роботу спеціалісту" activiti:candidateGroups="kuznets_mvk_3">
      <extensionElements>
        <activiti:formProperty id="orgName" name="Назва громади/організації" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventName" name="Назва заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventDate" name="Дата проведення заходу" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventPlace" name="Місце проведення заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventTime" name="Час проведення заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventEntertainment" name="Заход включатиме: " type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventSize" name="Очікувана кількість присутніх" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище організатора-громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я організатора-громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькове" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="dateCreation" name="Дата заповнення заяви" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="orgEmail" name="Емейл адреса організатора " type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="contactPhone" name="Контактний номер телефона" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="contactAddress" name="Контактна адреса організатора" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="needPhysicalCopy?" name="Форма відповіді" type="enum" writable="false"></activiti:formProperty>
        <activiti:formProperty id="workerDecision" name="Рішення чиновника" type="enum" required="true">
          <activiti:value id="accept" name="Надати послугу"></activiti:value>
          <activiti:value id="reject" name="Відмовити в послузі"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <userTask id="usertask2" name="Направлення відповіді громадянину" activiti:candidateGroups="kuznets_mvk_3">
      <extensionElements>
        <activiti:formProperty id="orgName" name="Назва громади/організації" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventName" name="Назва заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventDate" name="Дата проведення заходу" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventPlace" name="Місце проведення заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventTime" name="Час проведення заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventEntertainment" name="Заход включатиме: " type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventSize" name="Очікувана кількість присутніх" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище організатора-громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я організатора-громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькове" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="dateCreation" name="Дата заповнення заяви" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="orgEmail" name="Емейл адреса організатора " type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="contactPhone" name="Контактний номер телефона" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="contactAddress" name="Контактна адреса організатора" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="needPhysicalCopy?" name="Форма відповіді" type="enum" writable="false">
          <activiti:value id="digital" name="Тільки Електронна версія"></activiti:value>
          <activiti:value id="physical" name="Електронна + Фізична копія"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="additionalInfo" name="Додаткова інформація" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="scanCopy" name="Завантажьте скан-копію рішення (буде відправлено громадянину на e-mail" type="file"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="usertask3" name="Відповідь неможлива" activiti:candidateGroups="kuznets_mvk_3">
      <extensionElements>
        <activiti:formProperty id="refuseReason" name="Причина у відмові в наданні послуги" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="orgName" name="Назва громади/організації" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventName" name="Назва заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventDate" name="Дата проведення заходу" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventPlace" name="Місце проведення заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventTime" name="Час проведення заходу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventEntertainment" name="Заход включатиме: " type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="eventSize" name="Очікувана кількість присутніх" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище організатора-громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я організатора-громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькове" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="dateCreation" name="Дата заповнення заяви" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="orgEmail" name="Емейл адреса організатора " type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="contactPhone" name="Контактний номер телефона" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="contactAddress" name="Контактна адреса організатора" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="needPhysicalCopy?" name="Форма відповіді" type="enum" writable="false">
          <activiti:value id="digital" name="Тільки Електронна версія"></activiti:value>
          <activiti:value id="physcial" name="Фізична + Електронна версія"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="additionalInfo" name="Додаткова інформація" type="textArea"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" name="${workerDecision=='accept'}" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${workerDecision=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="${workerDecision=='reject'}" sourceRef="exclusivegateway1" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${workerDecision=='reject'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="mailtask3" name="Емейл відмова" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${orgEmail}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Відповідь щодо проведення заходу під назвою: ${eventName}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>Шановний громадянин

Нажаль ми не можемо дозволити проведення цього заходу з приведеними Вами деталями. 

Причина відмови - ${refuseReason}

Спробуйте заповнити заяву ще раз з врахуванням помилок. 

Перепрошуємо за цю незручність. 
Завжди ваша, МіськРада.</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow6" sourceRef="usertask2" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="usertask3" targetRef="mailtask3"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="servicetask1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="mailtask3" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="startevent1" targetRef="mailtask1"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="mailtask1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Емейл повідомлення" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>"${orgEmail}"</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Заява щодо проведення релігійного/культурного заходу</activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>"${scanCopy}"</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>Шановний громадянин

Ваша заява щодо проведення релігійного/культурного заходу була успішно опрацьована і задовілено. 

Деталі вашого заходу: 

Назва громади/організації - ${orgName}
Назва заходу - ${eventName}
Дата заходу - ${eventDate}
Місце проведення - ${eventPlace}
Час проведення - ${eventTime}
Кількість учасників - ${eventSize}
ПІП організатора - ${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}
Контактна інформація організатора - ${contactPhone} чи ${orgEmail}
Додаткове повідомлення від МіськРади - ${additionalInfo}

Якщо Ви побажали щоб вам було надано Фізичну копію документа то зверніться, буль ласка, до МіськРади за деталями того як, де і коли ії можно бути забрати. 

Завжди ваша, МіськРада.</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <textAnnotation id="textannotation1">
      <text>Відповідь готова</text>
    </textAnnotation>
    <textAnnotation id="textannotation2">
      <text>Неможливо дати відповідь</text>
    </textAnnotation>
  </process>
