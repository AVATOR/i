  <process id="myProcess" name="My process" isExecutable="true">
    <startEvent id="startevent1" name="Кліент хоче зарееструватися"></startEvent>
    <userTask id="registerForm" name="Заповнити форму з email-ом">
      <extensionElements>
        <activiti:formProperty id="$email" name="Email" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="$firstName" name="Імя" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="$Surname" name="Прізвище" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="registerForm"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="registerForm" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Incoming Data Validation/Verification ">
      <extensionElements>
        <activiti:field name="param1">
          <activiti:string>in</activiti:string>
        </activiti:field>
        <activiti:field name="param2">
          <activiti:string>out</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3" name="${param2=='true'}" sourceRef="exclusivegateway1" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${param2=='true'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" name="${param2=='false'}" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${param2=='false'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="regSuccess" name="Welcome fellow User!" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:string>$userEmail</activiti:string>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string>WebsiteAdmin</activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Welcome</activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:string>Welcome !!!!!!</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow5" sourceRef="usertask2" targetRef="registerForm"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="regSuccess"></sequenceFlow>
    <userTask id="usertask1" name="Add User to the database">
      <extensionElements>
        <activiti:formProperty id="$emailAddress" name="User's email" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="$firstName" name="User's name" type="String" required="true"></activiti:formProperty>
        <activiti:formProperty id="$Surname" name="User's Surname" type="String" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="usertask2" name="Ask User to re-fill Form"></userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="regSuccess" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow9" sourceRef="servicetask1" targetRef="exclusivegateway1"></sequenceFlow>
    <textAnnotation id="textannotation1">
      <text>Validation successful
</text>
    </textAnnotation>
    <textAnnotation id="textannotation2">
      <text>Mistake was made</text>
    </textAnnotation>
  </process>
