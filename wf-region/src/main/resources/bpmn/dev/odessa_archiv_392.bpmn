<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="myProcess" name="My process" isExecutable="true">
    <startEvent id="startevent1" name="Заява про підтвердження фактів народження, шлюбу, розірвання шлюбу, смерті">
      <extensionElements>
        <activiti:formProperty id="sPosition" name="Должность" type="string" default="Директору Державного архіву Одеської області" readable="false" writable="false" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameDirector" name="Прізвище директора архіву" type="string" default="Левчуку В.В." readable="false" writable="false" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім’я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Поштова адреса для зворотного зв’язку" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="privateMail" name="Електронна пошта (е-mail)" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="privatePhone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="esАpplication" name="Прошу видати архівну довідку про" type="enum" required="true">
          <activiti:value id="sEnumBirthDay" name="народження"></activiti:value>
          <activiti:value id="sEnumMarried" name="шлюб"></activiti:value>
          <activiti:value id="sEnumBreakMarried" name="розірвання шлюбу"></activiti:value>
          <activiti:value id="sEnumDead" name="смерть"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="lFullNameObject" name="П.І.Б. (прізвище (для жінок – обов’язково вказати дівоче прізвище), ім’я, по батькові народженого, одружених (розлучених), померлого)" type="label"></activiti:formProperty>
        <activiti:formProperty id="lastName_UA1" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="firstName_UA1" name="Ім’я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="middleName_UA1" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="dDateRegistration" name="Дата (рік) реєстрації (або дата події):" type="data" required="true"></activiti:formProperty>
        <activiti:formProperty id="aPlaceRegistration" name="Місце реєстрації (події) (населений пункт (місто, село); для дорадянського періоду: назва церкви, волость, повіт, губернія; для радянського періоду: район, область)" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="aCoustForDocument" name="Документ необхідний для" type="textArea" required="true"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Підтягування додаткових документів"></serviceTask>
    <serviceTask id="servicetask2" name="Інформування громадянина" activiti:async="true" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${privateMail}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>iGov: Заява про підтвердження фактів enum{[esАpplication]}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
										Ваш запит був успішно зареєстрований у нашій системі. 
								    &lt;/p&gt;
									&lt;p&gt;
										Спеціалісти оброблять Вашу заявку та надішлють Вам email з підтвердженням та запрошенням прийти у обраний час. 
									&lt;/p&gt;
									&lt;p&gt;[cancelTask]&lt;/p&gt; 
                                    [pattern/mail/_common_signature_start.html]
									[pattern_dictonary:dnepr_dms.csv:value{[viddil]}:2],&lt;br&gt;
									[pattern/mail/_common_signature_end.html]   
									[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="usertask1" name="Форма прийняття рішення">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім’я" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Поштова адреса для зворотного зв’язку" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="privateMail" name="Електронна пошта (е-mail)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="privatePhone" name="Контактний телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="esАpplication" name="Прошу видати архівну довідку про" type="enum" writable="false">
          <activiti:value id="sEnumBirthDay" name="народження"></activiti:value>
          <activiti:value id="sEnumMarried" name="шлюб"></activiti:value>
          <activiti:value id="sEnumBreakMarried" name="розірвання шлюбу"></activiti:value>
          <activiti:value id="sEnumDead" name="смерть"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="lFullNameObject" name="П.І.Б. (прізвище (для жінок – обов’язково вказати дівоче прізвище), ім’я, по батькові народженого, одружених (розлучених), померлого)" type="label"></activiti:formProperty>
        <activiti:formProperty id="lastName_UA1" name="Прізвище" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="firstName_UA1" name="Ім’я" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="middleName_UA1" name="По батькові" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="dDateRegistration" name="Дата (рік) реєстрації (або дата події):" type="data" writable="false"></activiti:formProperty>
        <activiti:formProperty id="aPlaceRegistration" name="Місце реєстрації (події) (населений пункт (місто, село); для дорадянського періоду: назва церкви, волость, повіт, губернія; для радянського періоду: район, область)" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="aCoustForDocument" name="Документ необхідний для" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="workerDecision" name="Результат обробки звернення" type="enum" required="true">
          <activiti:value id="accept" name="Сповістити громадянина про дату отримання довідки"></activiti:value>
          <activiti:value id="decline" name="Сповістити громадянина, що довідка не може бути надана"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="decisionComment" name="Зазначте дату отримання довідки або причину неможливості отримання послуги. Ця інформація буде відправлена на e-mail заявника *" type="textArea" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow2" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="servicetask1" targetRef="servicetask2"></sequenceFlow>
    <serviceTask id="servicetask3" name="Інформування посадовця" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${sPost}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове  звернення. Разова (індивідуальна) ліцензія на здійснення суб’єктами зовнішньоекономічної діяльності зовнішньоекономічних операцій]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;b&gt;Шановний колего!&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
У черзі на обробку зареєстровано нове звернення.&lt;br /&gt;
Заявник: ${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}&lt;br /&gt;
Опрацюйте це звернення на &lt;a href="http://region.igov.org.ua"&gt;порталі&lt;/a&gt;&lt;br /&gt;
[pattern/mail/_common_signature.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow5" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="servicetask4" name="Лист про відмову" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${privateMail}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>iGov: Заява про підтвердження фактів enum{[esАpplication]}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
										На жаль, Вам відмовлено у надані послуги.&lt;br /&gt;
										При необхідності отримати &lt;b&gt;Довідку про підтвердження фактів enum{[esАpplication]}&lt;/b&gt; Ви можете подати ще одну заявку на порталі &lt;a href="https://igov.org.ua/"&gt;iGov.org.ua&lt;/a&gt; врахувавши коментар Дуржавного сулжбовця
									&lt;/p&gt; 
									&lt;p&gt;
										Коментар Державного службовця:&lt;br&gt;
											&lt;b&gt;&lt;i&gt;${decisionComment}&lt;/i&gt;&lt;/b&gt;
									&lt;/p&gt;
									&lt;p&gt;
										При вниникненні питань Ви можете звернутися до чергового:&lt;br&gt;
										тел. [pattern_dictonary:dnepr_dms.csv:value{[viddil]}:5]
									&lt;/p&gt;
									[pattern/mail/_common_signature_start.html]
                                    [pattern_dictonary:dnepr_dms.csv:value{[viddil]}:2].&lt;/p&gt;
                                    [pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow7" name="відхилення" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${workerDecision=='decline'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask5" name="Лист про виконання" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${privateMail}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>iGov: Заява про підтвердження фактів enum{[esАpplication]}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
                                        Спеціалісти успішно опрацювли Вашу заяву.&lt;br /&gt;
						Ви можете отримати вашу &lt;b&gt;Довідку про підтвердження фактів enum{[esАpplication]}&lt;/b&gt;
за вдресою 

										Коментар Державного службовця:&lt;br&gt;
										&lt;b&gt;&lt;i&gt;${decisionComment}&lt;/i&gt;&lt;/b&gt;
									&lt;/p&gt;
									&lt;p&gt;
										При вниникненні питань Ви можете звернутися до чергового:&lt;br&gt;
										тел. [pattern_dictonary:dnepr_dms.csv:value{[viddil]}:5]
									&lt;/p&gt;
									&lt;p&gt;[cancelTask]&lt;/p&gt; 
                                    [pattern/mail/_common_signature_start.html]
									[pattern_dictonary:dnepr_dms.csv:value{[viddil]}:2],&lt;br&gt;
									[pattern/mail/_common_signature_end.html]   
									[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" name="документи ок" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${workerDecision=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="servicetask5" targetRef="endevent1"></sequenceFlow>
  </process>
