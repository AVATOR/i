        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Звернення на тему: ${topic}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>Шановний громадянин, ${bankIdfirstName} ${bankIdmiddleName}

${reasonCannotReply}</activiti:expression>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" sourceRef="cannotReply" targetRef="cannotReplyMail"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="cannotReplyMail" targetRef="endevent2"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="startevent2" targetRef="taskRecieved"></sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="emailConfirm" targetRef="exclusivegateway1"></sequenceFlow>
    <textAnnotation id="textannotation1">
      <text>Відповідь готова</text>
    </textAnnotation>
    <textAnnotation id="textannotation2">
      <text>Неможливо дати відповідь</text>
    </textAnnotation>
