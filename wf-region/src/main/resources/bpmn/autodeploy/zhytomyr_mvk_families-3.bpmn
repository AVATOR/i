<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="zhytomyr_mvk_families-3" name="Надання довідки про те, що за місцем реєстрації батька або матері посвідчення батьків та дитини з багатодітної сім'ї не видавались" isExecutable="true">
    <documentation>https://github.com/e-government-ua/iBP/issues/322</documentation>
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькові" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні данні" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="surname" name="Прізвище родини в родовому відмінку;наприклад, &quot;Іванових&quot;" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="adres_1" name="Адреса реєстрації громадянина" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="adres_2" name="Адреса проживання родини" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Укажіть контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="passport_father" name="Копія паспорту батька (сторінки 1, 2 та прописка)" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="passport_mother" name="Копія паспорту матері (сторінки 1, 2 та прописка)" type="file" required="true"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <userTask id="usertask1" name="опрацювання звернення" activiti:candidateGroups="zhitomir_mvk_family">
      <extensionElements>
        <activiti:formProperty id="clfio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні данні" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="surname" name="Прізвище родини в родовому відмінку;наприклад, &quot;Іванових&quot;" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="adres_1" name="Адреса реєстрації громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="adres_2" name="Адреса проживання родини" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Укажіть контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="decision_2" name="прийняте рішення" type="enum" required="true">
          <activiti:value id="accept" name="Довідка створена, запросити громадянина в ЦНАП для отримання"></activiti:value>
          <activiti:value id="reject" name="Відхилити звернення (обов'язково впишіть коментар нижче)"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment_1" name="Додаткова інформація громадянину у разі надання довідки" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="comment_2" name="У разі відхилення вкажіть, будь ласка, причину" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="PrintForm_1" name="File label;File title;pattern/print/zhitomir/zhytomyr_mvk_families_3.html" type="file"></activiti:formProperty>
        <activiti:formProperty id="sBody_1" name="[pattern/print/zhitomir/zhytomyr_mvk_families_3.html]" type="invisible" default="заява" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${passport_father},${passport_mother}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <serviceTask id="servicetask4" name="повідомлення громадянина про можливість отримання довідки" activiti:delegateExpression="#{MailTaskWithoutAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>"${email}"</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Звернення на тему: Надання довідки про те, що за місцем реєстрації батька або матері посвідчення батьків та дитини з багатодітної сім'ї не видавались</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
Ви подавали звернення щодо отримання довідки довідки про те, що за місцем реєстрації батька або матері посвідчення батьків та дитини з багатодітної сім'ї не видавались. Довідка створена і Ви можете отримати її за адресою м.Житомир, вул.Михайлівська, 4. Графік роботи спеціаліста, що займається видачаю даних довідок: вівторок з 9:00 до 13:00, четвер з 14:00 до 18:00. Для отримання довідки з собою потрібно мати паспорти батька та матері.&lt;br /&gt;&lt;br /&gt;

${comment_1}&lt;br /&gt;
[pattern/mail/_common_signature_start.html]
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_feedback.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow26" sourceRef="servicetask4" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow33" sourceRef="boundaryerror1" targetRef="endevent3"></sequenceFlow>
    <serviceTask id="servicetask5" name="Повідомлення співробітника про нову заявку" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>9173519@gmail.com</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Нове  звернення. Надання довідки про те, що за місцем реєстрації батька або матері посвідчення батьків та дитини з багатодітної сім'ї не видавались</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
Шановний(-а) колего!&lt;br /&gt;&lt;br /&gt;
У Вас в черзі на обробку зареєстровано нове звернення.&lt;br /&gt;
Заявник: ${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}&lt;br /&gt;
Опрацюйте це звернення на &lt;a href="https://region.igov.org.ua" target="_blank"&gt;порталі&lt;/a&gt;&lt;br /&gt;
[pattern/mail/_common_signature.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask6" name="повідомлення громадянина про відхилення звернення" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>"${email}"</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Звернення на тему: Надання довідки про те, що за місцем реєстрації батька або матері посвідчення батьків та дитини з багатодітної сім'ї не видавались</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
На жаль, Ваше звернення щодо надання довідки про те, що за місцем реєстрації батька або матері посвідчення батьків та дитини з багатодітної сім'ї не видавались, відхилено.&lt;br /&gt;
Причина в наступному: ${comment_2}&lt;br /&gt;
Звертаємо увагу, Ви можете заповнити заявку повторно на порталі &lt;a href="https://igov.org.ua/" target="_blank"&gt;iGov.org.ua&lt;/a&gt;
[pattern/mail/_common_signature_start.html]
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_feedback.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask1" name="Завантаження файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow37" sourceRef="servicetask5" targetRef="usertask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow39" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow40" sourceRef="exclusivegateway1" targetRef="servicetask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_2=='reject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow41" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_2=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow43" sourceRef="servicetask6" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow44" sourceRef="servicetask1" targetRef="servicetask5"></sequenceFlow>
    <sequenceFlow id="flow45" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
  </process>
