<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="dnepr_mvd_199" name="Дніпропетровськ - МВД - відстрілювання вогнепальної зброї та огляд її технічного стану" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По Батькові" type="string"></activiti:formProperty>
        <activiti:formProperty id="date_today" name="Укажіть сьогоднішню дату" type="date" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Електронна адреса для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="weapon_type" name="Тип зброї" type="enum" required="true">
          <activiti:value id="rifle" name="нарізна"></activiti:value>
          <activiti:value id="traumatic" name="травматична"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="weapon" name="Найменування зброї" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="weapon_number" name="Заводський номер зброї" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="weapon_caliber" name="Калібр зброї" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sID_Payment" name="ИД транзакции платежа" type="invisible" default=" "></activiti:formProperty>
        <activiti:formProperty id="nID_Subject" name="ИД-номер субъекта" type="invisible" default="0"></activiti:formProperty>
        <activiti:formProperty id="sID_Merchant" name="ИД-строковой мерчанта (магазина)" type="invisible" default="i14273228627"></activiti:formProperty>
        <activiti:formProperty id="sSum" name="сумма платежа" type="invisible" default="85.94"></activiti:formProperty>
        <activiti:formProperty id="sID_Currency" name="ИД-строковой валюты" type="invisible" default="UAH"></activiti:formProperty>
        <activiti:formProperty id="sDescription" name="строка-описание платежа" type="invisible" default=";04000043; 036032;1;відстрілювання вогнепальної зброї та огляд її технічного стану: нарізної і комбінованої мисливської та спортивної зброї. (у т.ч. коміссія 1.8%)"></activiti:formProperty>
       <activiti:formProperty id="rName" name="номер району" type="invisible"></activiti:formProperty>
       <activiti:formProperty id="markers12" name="маркер нарізна" type="markers" default="{&quot;motion&quot;:{&quot;ValuesFieldsOnCondition_12&quot;:{&quot;aField_ID&quot;:[&quot;rName&quot;], &quot;asID_Field_sValue&quot;:[&quot; &quot;],&quot;asID_Field&quot;:{&quot;sReason&quot;:&quot;weapon_type&quot;},&quot;sCondition&quot;:&quot;[sReason]=='rifle'&quot;}}}"></activiti:formProperty>
       <activiti:formProperty id="markers13" name="маркер травматична" type="markers" default="{&quot;motion&quot;:{&quot;ValuesFieldsOnCondition_13&quot;:{&quot;aField_ID&quot;:[&quot;rName&quot;], &quot;asID_Field_sValue&quot;:[&quot;Про можливість руйнування пристрою внаслідок використання наданих мною патронів, попереджений.&quot;],&quot;asID_Field&quot;:{&quot;sReason&quot;:&quot;weapon_type&quot;},&quot;sCondition&quot;:&quot;[sReason]=='traumatic'&quot;}}}"></activiti:formProperty>
       </extensionElements>
    </startEvent>
    <userTask id="usertask1" name="обробка звернення" activiti:candidateGroups="dnepr_mvd_199">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="date_today" name="Укажіть сьогоднішню дату" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="weapon_type" name="Тип зброї" type="enum" writable="false">
          <activiti:value id="rifle" name="нарізна"></activiti:value>
          <activiti:value id="traumatic" name="травматична"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="weapon" name="Найменування зброї" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="weapon_number" name="Заводський номер зброї" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="weapon_caliber" name="Калібр зброї" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="PrintForm_1" name="File label;File title;pattern/print/dnepr_mvd_199.html" type="file"></activiti:formProperty>
        <activiti:formProperty id="sBody_1" name="[pattern/print/dnepr_mvd_199.html]" type="invisible" default="заява громадянина" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="servicetask3" name="Завантаження файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <sequenceFlow id="flow5" sourceRef="startevent1" targetRef="servicetask3"></sequenceFlow>
    <serviceTask id="servicetask4" name="повідомлення громадянина" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[test_noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA["${email}"]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: відстрілювання вогнепальної зброї та огляд її технічного стану]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.<br /><br />
Ви подали звернення на відстрілювання вогнепальної зброї та огляд її технічного стану.<br/>
Далі Вам необхідно:<br />
1. Роздрукувати заяву у додатку до листа, поставити на ній свій підпис.<br />
2. Здійснити оплату за відстрілювання зброї у розмірі 85 грн 94 коп. Ви можете це зробити в електронному варіанті натиснувши цю кнопку<br />
[paymentButton_LiqPay]
або іншим зручним способом. За результатами Ви повинні мати на руках квитанцію про сплату.<br />
3. Відвідати науково-дослідний експертно-криміналістичний <a href="http://ndekcmvd.dp.ua/" target="_blank">центр</a> при ГУМВС України. Окрім підписаної Вами заяви та квитанції про сплату при собі треба мати:<br />
4. Дозвіл (постійний або тимчасовий) на володіння зброєю.<br />
5. Зброю та 4 патрони для відстрілювання (для револьверів треба надати по 1 патрону на кожну камеру в барабані).<br />
<br />
Графік роботи науково-дослідного експертно-криміналістичного центру:<br />
понеділок-п’ятниця с 9:30 до 17:30.<br />
Перерва с 13:00 до 14:00<br />
Адреса: вул. Будівельний тупік, 1а <a href="http://ndekcmvd.dp.ua/images/p004_0_01_01.jpg" target="_blank">(карта)</a>.<br/>
[pattern/mail/_common_signature_start.html]
 НДЕКЦ при ГУМВС України <br />
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_feedback.html]
[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[${PrintForm_1}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow7" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="usertask1" targetRef="servicetask4"></sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_dnepr_mvd_199">
    <bpmndi:BPMNPlane bpmnElement="dnepr_mvd_199" id="BPMNPlane_dnepr_mvd_199">
      <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="119.0" y="50.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask1" id="BPMNShape_usertask1">
        <omgdc:Bounds height="55.0" width="105.0" x="170.0" y="130.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="75.0" y="74.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="servicetask3" id="BPMNShape_servicetask3">
        <omgdc:Bounds height="55.0" width="105.0" x="170.0" y="40.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="servicetask4" id="BPMNShape_servicetask4">
        <omgdc:Bounds height="55.0" width="105.0" x="40.0" y="130.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="154.0" y="67.0"></omgdi:waypoint>
        <omgdi:waypoint x="170.0" y="67.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow7" id="BPMNEdge_flow7">
        <omgdi:waypoint x="92.0" y="130.0"></omgdi:waypoint>
        <omgdi:waypoint x="92.0" y="109.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="222.0" y="95.0"></omgdi:waypoint>
        <omgdi:waypoint x="222.0" y="130.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="170.0" y="157.0"></omgdi:waypoint>
        <omgdi:waypoint x="145.0" y="157.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>