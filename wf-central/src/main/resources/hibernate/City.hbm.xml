<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.wf.dp.dniprorada">
    <class name="org.wf.dp.dniprorada.model.City" table="City">
        <id name="id" column="nID">
            <generator class="native"/>
        </id>

        <property name="name" column="sName" not-null="true"/>
        <many-to-one name="region" column="nID_Region" not-null="true" lazy="false"/>
    </class>
</hibernate-mapping>